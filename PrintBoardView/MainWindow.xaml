        <TextBox Name="FileNameTextBox" HorizontalAlignment="Left" TextAlignment="Center" Height="23" Margin="217,112,0,0" TextWrapping="Wrap" Text="Open a Circuit" VerticalAlignment="Top" Width="371"/>
